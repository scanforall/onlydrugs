              li.menu__item
                a.menu__link(href="../listen")
                  .menu__text LISTEN
              li.menu__item
                a.menu__link(href="../video")
                  .menu__text VIDEO
              li.menu__item
                a.menu__link(href="../contact")
                  .menu__text CONTACT
